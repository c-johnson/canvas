{
  "name": "@canvas-js/store",
  "version": "v0.5.0-alpha3",
  "author": "Canvas Technologies, Inc. (https://canvas.xyz)",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "lib",
    "protocols"
  ],
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "imports": {
    "#protocols/sync": "./protocols/sync/index.cjs",
    "#protocols/discovery": "./protocols/discovery/index.cjs",
    "#sync": "./lib/sync/index.js"
  },
  "exports": {
    ".": "./lib/index.js",
    "./constants": "./lib/constants.js",
    "./bootstrap": "./lib/bootstrap.js",
    "./service/browser": "./lib/service/browser/index.js",
    "./service/node": "./lib/service/node/index.js"
  },
  "scripts": {
    "generate": "npm run generate-sync-js && npm run generate-sync-ts",
    "generate-sync-js": "pbjs ./protocols/sync/index.proto -t static-module -w commonjs -o ./protocols/sync/index.cjs",
    "generate-sync-ts": "pbjs -t static-module ./protocols/sync/index.proto | pbts -o ./protocols/sync/index.d.cts -",
    "test": "ava"
  },
  "dependencies": {
