    "it-pipe": "^3.0.1",
    "it-stream-types": "^2.0.1",
    "libp2p": "^0.45.3",
    "microcbor": "^0.2.1",
    "p-queue": "^7.3.4",
    "protobufjs": "^7.2.3",
    "uint8arraylist": "^2.4.3",
    "uint8arrays": "^4.0.3"
  },
  "devDependencies": {
    "@ava/typescript": "^4.0.0",
    "@types/node": "^20.1.5",
    "ava": "^5.2.0",
