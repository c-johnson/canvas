{
  "name": "@canvas-js/store",
  "version": "0.4.11",
  "author": "Canvas Technologies, Inc. (https://canvas.xyz)",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "lib",
    "rpc",
    "sqlite",
    "types"
  ],
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "imports": {
    "#protocols/sync": "./protocols/sync/index.cjs",
    "#protocols/discovery": "./protocols/discovery/index.cjs"
  },
  "scripts": {
    "generate": "npm run generate-sync && npm run generate-discovery",
    "generate-sync": "npm run generate-sync-js && npm run generate-sync-ts",
    "generate-sync-js": "pbjs ./protocols/sync/index.proto -t static-module -w commonjs -o ./protocols/sync/index.cjs",
    "generate-sync-ts": "pbjs -t static-module ./protocols/sync/index.proto | pbts -o ./protocols/sync/index.d.cts -",
    "generate-discovery": "npm run generate-discovery-js && npm run generate-discovery-ts",
    "generate-discovery-js": "pbjs ./protocols/discovery/index.proto -t static-module -w commonjs -o ./protocols/discovery/index.cjs",
    "generate-discovery-ts": "pbjs -t static-module ./protocols/discovery/index.proto | pbts -o ./protocols/discovery/index.d.cts -",
    "test": "ava"
  },
  "dependencies": {
    "@canvas-js/okra": "^0.0.1",
    "@canvas-js/okra-idb": "^0.0.2",
    "@canvas-js/okra-memory": "^0.0.1",
    "@canvas-js/okra-node": "^0.3.1",
    "@chainsafe/libp2p-gossipsub": "^7.0.0",
    "@chainsafe/libp2p-noise": "^11.0.4",
    "@libp2p/bootstrap": "^6.0.3",
    "@libp2p/interface-connection": "^4.0.0",
    "@libp2p/interface-peer-id": "^2.0.1",
    "@libp2p/interfaces": "^3.3.1",
    "@libp2p/logger": "^2.0.7",
    "@libp2p/mplex": "^8.0.1",
    "@libp2p/peer-id": "^2.0.3",
    "@libp2p/peer-id-factory": "^2.0.3",
    "@libp2p/websockets": "^6.0.0",
    "@multiformats/multiaddr": "^12.1.2",
    "@noble/hashes": "^1.3.0",
    "any-signal": "^4.1.1",
    "big-varint": "^0.1.3",
    "it-length-prefixed": "^9.0.1",
    "it-pipe": "^3.0.1",
    "it-stream-types": "^2.0.1",
    "libp2p": "0.44.0",
    "microcbor": "^0.2.1",
    "p-queue": "^7.3.4",
    "protobufjs": "^7.2.3",
    "uint8arraylist": "^2.4.3",
    "uint8arrays": "^4.0.3"
  },
  "devDependencies": {
    "@ava/typescript": "^4.0.0",
    "@types/node": "^18.15.11",
    "ava": "^5.2.0",
    "nanoid": "^4.0.2",
    "protobufjs-cli": "^1.1.1",
    "typescript": "^5.0.4"
  }
}
